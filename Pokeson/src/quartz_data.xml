<!-- <?xml version="1.0" encoding="UTF-8"?> -->
<job-scheduling-data version="1.8" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://www.quartz-scheduler.org/xml/JobSchedulingData" >
   
   <schedule>
        <job>
            <name>TaskRun</name>
            <job-class>TaskRun</job-class>
        </job>
        <trigger>
            <cron>
                <name>TaskRun</name>
                <job-name>TaskRun</job-name>
                <cron-expression>20 45 17 ? * *</cron-expression>
<!--                 0 40 16 ? * THU -->
            </cron>
        </trigger>
    </schedule>
 
</job-scheduling-data>
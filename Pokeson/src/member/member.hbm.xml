<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="member.MemberVO" table="member">
		<id name="member_no" column="member_no">
			<generator class ="identity"/>
		</id>
		
		<property name="member_name" column="member_name"/>
		<property name="member_id" column="member_id"/>
		<property name="member_password" column="member_password"/>
		<property name="member_phone" column="member_phone"/>
		<property name="member_address" column="member_address"/>
		<property name="member_gender" column="member_gender"/>
		<property name="member_Email" column="member_Email"/>
		<property name="member_birthday" column="member_birthday"/>
		<property name="member_bonus" column="member_bonus"/>
		<property name="member_GoogleId" column="member_GoogleId"/>
		<property name="member_state" column="member_state"/>
		
	</class>
</hibernate-mapping>